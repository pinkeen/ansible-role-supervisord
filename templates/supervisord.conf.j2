[supervisord]
http_port=/var/tmp/supervisor.sock
logfile=/var/log/supervisor/supervisord.log
logfile_maxbytes=50MB       
logfile_backups=10          
loglevel=info               
pidfile=/var/run/supervisord.pid 
nodaemon=false              
minfds=1024                 
minprocs=200                

[supervisorctl]
serverurl=unix:///var/tmp/supervisor.sock

[include]
files = /etc/supervisord.d/*.conf
